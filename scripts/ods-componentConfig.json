{
  "source": [ "./src/ods-shape.json" ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "./src/tokens/",
      "files": [
        {
          "destination": "_ods-componentShapeVariables.scss",
          "format": "scss/variables"
        }
      ]
    },
    "css": {
      "transformGroup": "css",
      "buildPath": "./src/tokens/",
      "files": [
        {
          "destination": "ods-componentShapeProperties.scss",
          "format": "css/variables"
        }
      ]
    }
  }
}
